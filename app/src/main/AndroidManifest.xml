<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jammind">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-feature
        android:name="android.hardware.Camera"
        android:required="true" />

    <application
        android:name=".Configuracao"
        android:allowBackup="true"
        android:icon="@mipmap/nova_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/nova_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".SobreActivity"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".menuAluno.salas.ListaHistoricoDoAlunoActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuProfessor.salas.HistoricoAlunoActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuAluno.mural.ExibirMuralActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuAluno.salas.ResponderTarefasActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuAluno.salas.ExibirTarefasActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuAluno.salas.ExibirSalaActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuAluno.salas.TemasAlunosActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:screenOrientation="portrait"
            android:name=".menuAluno.MenuAlunoActivity"
            android:label="@string/title_activity_menu_aluno"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".menuProfessor.salas.LiberarTarefaSalaActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuProfessor.salas.ListaAlunosSalasActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuProfessor.salas.SalaProfessorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuProfessor.salas.ListarSalasProfessorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".menuProfessor.salas.CriarSalaActivity"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".menuProfessor.MenuProfessorActivity"
            android:label="Principal"
            android:theme="@style/AppTheme.NoActionBar"
            android:screenOrientation="portrait"/>
        <activity android:name=".formulario.FormularioAlunoActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".formulario.FormularioProfessorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>